{
    "expo": {
        "name": "petition-pledge",
        "slug": "petition-pledge",
        "version": "1.0.5",
        "orientation": "portrait",
        "icon": "./assets/icon.png",
        "splash": {
            "image": "./assets/splash.png",
            "resizeMode": "contain",
            "backgroundColor": "#ffffff"
        },
        "updates": {
            "fallbackToCacheTimeout": 0
        },
        "assetBundlePatterns": ["**/*"],
        "ios": {
            "bundleIdentifier": "com.timg512372.petition-pledge",
            "requireFullScreen": true,
            "usesAppleSignIn": true
        },
        "android": {
            "versionCode": 1,
            "permissions": ["CAMERA_ROLL", "CONTACTS"]
        }
    }
}
